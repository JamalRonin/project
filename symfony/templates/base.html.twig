<!DOCTYPE html>
<html>
	<head>
		<!-- meta tag -->
		<meta charset="UTF-8">
		<title>
			{% block title %}Accueil
			{% endblock %}
		</title>
		<meta
		name="description" content="">
		<!-- responsive tag -->
		<meta http-equiv="x-ua-compatible" content="ie=edge">
		<meta
		name="viewport" content="width=device-width, initial-scale=1">
		<!-- favicon -->
		<link rel="apple-touch-icon" href="apple-touch-icon.png">
		<link
		rel="shortcut icon" type="image/x-icon" href="{{asset('/images/fav.png')}}">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{# {{ encore_entry_link_tags('app') }} #}
			<link rel="stylesheet" type="text/css" href="{{ asset('css/bootstrap.min.css') }}">
			<link rel="stylesheet" href="{{ asset('css/font-awesome.min.css') }}">
			<link rel="stylesheet" href="{{ asset('css/uicons-regular-rounded.css') }}">
			<link rel="stylesheet" href="{{ asset('fonts/flaticon.css') }}">
			<link rel="stylesheet" href="{{ asset('css/animate.css') }}">
			<link rel="stylesheet" href="{{ asset('css/owl.carousel.css') }}">
			<link rel="stylesheet" href="{{ asset('css/off-canvas.css') }}">
			<link rel="stylesheet" href="{{ asset('css/magnific-popup.css') }}">
			<link rel="stylesheet" href="{{ asset('css/rsmenu-main.css') }}">
			<link rel="stylesheet" href="{{ asset('css/rs-spacing.css') }}">
			<link rel="stylesheet" href="{{ asset('css/mauticForm.css') }}">
			<link rel="stylesheet" href="{{ asset('css/style.css') }}">
			<!-- Template Main CSS File -->
			<link rel="stylesheet" href="{{ asset('css/responsive.css') }}">
		{% endblock %}
	</head>

	<body class="home-primary-color">
		<div class="offwrap"></div>
		{% include "partials/preloader.html.twig" %}
		<!-- Main content Start -->
		<div class="main-content">
			{% include "partials/header.html.twig" with {'realisations' : realisations , 'services' : services } %}
			{% block body %}{% endblock %}
			{% block partner %}
				<!-- Partner Start -->
				{% include "partials/partner.html.twig" %}
				<!-- Partner End -->
			{% endblock %}
		</div>
		<!-- Main content End -->
		{% include "partials/footer.html.twig" %}
		{% include "partials/scrollup.html.twig" %}
		{% include "partials/search.html.twig" %}
		{% block javascripts %}
			{# {{ encore_entry_script_tags('app') }} #}
			<!-- modernizr js --><script src="{{ asset('js/modernizr-2.8.3.min.js')}}"> </script>
			<!-- jquery latest version -->
			<script src="{{ asset('js/jquery.min.js')}}"></script>
			<!-- Bootstrap v4.4.1 js -->
			<script src="{{ asset('js/bootstrap.min.js')}}"></script>
			<!-- op nav js-->
			<script src="{{ asset('js/jquery.nav.js')}}"></script>
			<!-- owl.carousel js -->
			<script src="{{ asset('js/owl.carousel.min.js')}}"></script>
			<!-- wow js -->
			<script src="{{ asset('js/wow.min.js')}}"></script>
			<!-- Skill bar js -->
			<script src="{{ asset('js/skill.bars.jquery.js')}}"></script>
			<!-- imagesloaded js -->
			<script src="{{ asset('js/imagesloaded.pkgd.min.js')}}"></script>
			<!-- waypoints.min.js -->
			<script src="{{ asset('js/waypoints.min.js')}}"></script>
			<!-- counterup.min js -->
			<script src="{{ asset('js/jquery.counterup.min.js')}}"></script>
			<!-- magnific popup js -->
			<script src="{{ asset('js/jquery.magnific-popup.min.js')}}"></script>
			<script src="{{ asset('js/jquery.easypiechart.min.js')}}"></script>
			<!-- contact form js -->
			<script src="{{ asset('js/contact.form.js')}}"></script>
			<!-- Mautic form js -->
			<script src="{{ asset('js/mauticForm.js')}}"></script>		
			<!-- main js -->
			<script src="{{ asset('js/main.js')}}"></script>
		{% endblock %}
		<script>
			(function (w, d, t, u, n, a, m) {
				w['MauticTrackingObject'] = n;
				w[n] = w[n] || function () {
				(w[n].q = w[n].q || []).push(arguments)
				},
				a = d.createElement(t),
				m = d.getElementsByTagName(t)[0];
				a.async = 1;
				a.src = u;
				m.parentNode.insertBefore(a, m)
			})
			(window, document, 'script', 'http://mautic.net-et.sur.com.yjbt3669.odns.fr/mtc.js', 'mt');
			mt('send', 'pageview');
		</script>
		<script type="text/javascript" src="https://cdn.jsdelivr.net/npm/lozad/dist/lozad.min.js"></script>
<script>
  var lazyLoadInstance = new LazyLoad({
    elements_selector: ".lazy"
  });
</script>
	</body>
</html>

